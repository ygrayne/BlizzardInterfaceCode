<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<Frame name="UIWidgetTemplateTextureWithState" inherits="UIWidgetBaseTemplate" secureMixin="UIWidgetTemplateTextureWithStateMixin" virtual="true">
		<Size x="451" y="301"/>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Background">
					<Size x="451" y="301"/> 
					<Anchors>
						<Anchor point="CENTER"/> 
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture parentKey="Portrait" useAtlasSize="true">
					<Size x="70" y="100"/>
					<Anchors>
						<Anchor point="RIGHT" relativeKey="$parent.Background" x="-20" y="0"/>
					</Anchors>
				</Texture>
				<FontString parentKey="Text" inherits="GameFontNormalLarge">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>
